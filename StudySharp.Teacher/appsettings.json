{
  "ConnectionStrings": {
    "Default": "Server={pgHost};Port={pgPort};User Id={pgUserId};Password={pgPassword};Database={pgDatabase}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "jwtTokenConfig": {
    "secret": "1234567890123456789",
    "issuer": "issuer",
    "audience": "audience",
    "accessTokenExpiration": 20,
    "refreshTokenExpiration": 60
  }
}
